	make clean -f makeall.lnx CONFIG=Debug APP=DrawImplicitSurface
	make clean -f makeall.lnx CONFIG=Debug APP=MapTextureToQuad
	make clean -f makeall.lnx CONFIG=Debug APP=Triangulation
	make clean -f makeall.lnx CONFIG=Release APP=Boolean2D
	make clean -f makeall.lnx CONFIG=Release APP=BSplineLeastSquares
	make clean -f makeall.lnx CONFIG=Release APP=ClodPolyline
	make clean -f makeall.lnx CONFIG=Release APP=DrawImplicitSurface
